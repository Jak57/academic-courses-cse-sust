        "############\n",
        "#  Linear Convolution\n",
        "############\n",
        "\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "x = [1, 2, 3, 1]\n",
        "h = [1, 2, 1, -1]\n",
        "y = []\n",
        "\n",
        "for i in range(len(x)+len(h)-1):\n",
        "  y.append(0)\n",
        "\n",
        "for i in range(len(x)):\n",
        "  for j in range(len(h)):\n",
        "    y[i+j] += (x[i]*h[j])\n",
        "\n",
        "plt.stem(y)\n"
      ]
    }
